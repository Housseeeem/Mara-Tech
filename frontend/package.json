{
  "scripts": {
    "build:css": "npx tailwindcss -i ./input.css -o ./output.css",
    "watch:css": "npx tailwindcss -i ./input.css -o ./output.css --watch",
    "serve": "npx http-server -p 3001 -c-1",
    "start": "npx concurrently \"npm run watch:css\" \"npm run serve\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "http-server": "^14.1.1",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_mesh": "^0.4.1633559619"
  }
}
